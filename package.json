{
  "name": "reports-api",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.json . && stylelint **/*.css",
    "start": "NODE_PATH=. bin/www",
    "debug": "NODE_PATH=. nodemon --trace-warnings --inspect bin/www",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "test": "NODE_PATH=. TZ=UTC jest --silent --testPathPattern app/",
    "test:functional": "NODE_PATH=. jest --detectOpenHandles --forceExit test/functional/*"
  },
  "engines": {
    "node": "14.15.4"
  },
  "dependencies": {
    "@babel/core": "~7.9.0",
    "@babel/plugin-transform-modules-commonjs": "~7.9.0",
    "@babel/preset-react": "~7.9.0",
    "@babel/register": "~7.9.0",
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.5",
    "cors": "~2.8.5",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "express-json-validator-middleware": "~2.1.1",
    "http-errors": "~1.7.3",
    "i18next": "~19.3.3",
    "i18next-sync-fs-backend": "~1.1.1",
    "knex": "~0.20.13",
    "lodash": "~4.17.20",
    "morgan": "~1.10.0",
    "nconf": "~0.10.0",
    "objection": "~2.1.3",
    "pg": "^8.0.3",
    "swagger-ui-express": "~4.1.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-loader": "~8.1.0",
    "common-tags": "~1.8.0",
    "eslint": "~7.11.0",
    "eslint-config-prettier": "~6.12.0",
    "eslint-plugin-i18next": "~5.0.0",
    "eslint-plugin-json": "~2.1.2",
    "eslint-plugin-prettier": "~3.1.4",
    "eslint-plugin-react": "~7.21.3",
    "jest": "~26.5.2",
    "jest-json-schema": "~2.1.0",
    "nock": "~13.0.4",
    "nodemon": "~2.0.4",
    "prettier": "~2.1.2",
    "stylelint": "~13.7.2",
    "stylelint-config-standard": "~20.0.0",
    "supertest": "~5.0.0",
    "timekeeper": "~2.2.0"
  }
}
